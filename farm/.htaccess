RewriteEngine On

RewriteCond %{REQUEST_URI} !^/?dokuwiki/
#RewriteCond %{REQUEST_URI} !^/?farm/
RewriteRule ^/?([^/]+)/(.*) /dokuwiki/$2?animal=$1 [QSA,L]
RewriteCond %{REQUEST_URI} !^/?dokuwiki$
#RewriteCond %{REQUEST_URI} !^/?farm$
RewriteRule ^/?([^/]+)$ /dokuwiki/?animal=$1 [QSA]

Options +FollowSymLinks
